{
  "name": "unocss-merge",
  "version": "0.1.0",
  "description": "simple utility to merge unocss class names",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./react": {
      "types": "./dist/react.d.ts",
      "default": "./dist/react.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "prepublishOnly": "pnpm typecheck && pnpm test-cover && pnpm build"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.4",
    "@swc/core": "^1.11.22",
    "@types/node": "^22.15.0",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitest/coverage-v8": "^3.1.2",
    "es-toolkit": "^1.36.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tsdown": "^0.9.8",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/unocss-merge.git"
  },
  "keywords": [
    "unocss",
    "merge",
    "tw-merge"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/unocss-merge/issues"
  },
  "homepage": "https://github.com/magicdawn/unocss-merge#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx,less,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
